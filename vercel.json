{
    "$schema": "https://openapi.vercel.sh/vercel.json",
    "version": 2,
    "devCommand": "npm run dev",
    "buildCommand": "npm run build",
    "outputDirectory": "dist",
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/node"
        },
        {
            "use": "@vercel/static-build",
            "config": {
                "distDir": "public"
            }
        }
    ]
}